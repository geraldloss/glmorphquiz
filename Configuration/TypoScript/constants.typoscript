		# cat=plugin.tx_glmorphquiz/file; type=string; label=Path to template partials (FE)
		partialRootPath = EXT:glmorphquiz/Resources/Private/Partials/
		# cat=plugin.tx_glmorphquiz/file; type=string; label=Path to template layouts (FE)
		layoutRootPath = EXT:glmorphquiz/Resources/Private/Layouts/
	}
	libraries{
		#cat=plugin.tx_glcrossword/libraries; type=boolean; label=LLL:EXT:glcrossword/locallang.xlf:glcrossword.constants.inlcudeJQuery
		inlcudeJQuery = true
	}
}
